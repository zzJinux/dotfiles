[url "ssh://git@git.workplace.in/"]
  insteadOf = https://git.workplace.in/
[includeIf "gitdir:~/workplacerepos/"]
  path = ~/.config/git/config_workplace__local
[alias]
  devtag = "!f(){ tagname=dev-${1:+$1-}$(date +%y%m%d%H%M)-kst-$(git rev-parse --short HEAD);git tag -a $tagname -m \"Creator: $(git config user.name)\" && echo $tagname;};f"
