{
    "workbench.settings.applyToAllProfiles": [
        "workbench.list.horizontalScrolling",
        "window.newWindowProfile",
        "files.autoSaveDelay",
        "files.autoSave",
        "editor.fontFamily",
        "editor.fontSize",
        "editor.inlineSuggest.enabled",
        "editor.mouseWheelZoom",
        "editor.find.seedSearchStringFromSelection",
        "[shellscript]",
        "[python]",
        "python.testing.pytestEnabled",
        "python.analysis.autoImportCompletions",
        "python.analysis.fixAll",
        "python.analysis.typeCheckingMode",
        "files.associations",
        "terminal.integrated.fontFamily",
        "terminal.integrated.fontSize",
        "terminal.integrated.scrollback",
        "terminal.integrated.defaultProfile.osx",
        "terminal.external.osxExec",
        "git.autofetch",
        "gitlens.ai.model",
        "gitlens.ai.gitkraken.model",
        "redhat.telemetry.enabled",
        "vim.sneak",
        "vim.smartRelativeLine",
        "workbench.iconTheme",
        "workbench.colorCustomizations",
        "editor.tokenColorCustomizations",
        "workbench.colorTheme",
        "cursor.composer.shouldAllowCustomModes",
        "cursor.composer.shouldChimeAfterChatFinishes",
    ],
    "workbench.list.horizontalScrolling": true,
    "window.newWindowProfile": "Default",
    "files.autoSaveDelay": 100,
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, Hack, Monaco, Menlo, Courier New, monospace",
    "editor.fontSize": 13,
    "editor.inlineSuggest.enabled": true,
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.mouseWheelZoom": true,
    "[shellscript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "mkhl.shfmt"
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports.ruff": "always"
        }
    },
    "python.testing.pytestEnabled": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.fixAll": [
        "source.unusedImports"
    ],
    "python.analysis.typeCheckingMode": "standard",
    "files.associations": {
        "**/.git/info/exclude": "ignore",
        ".env*": "dotenv",
        "ssh_config": "ssh_config"
    },
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, Hack, Monaco, Menlo, Courier New, monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.scrollback": 3000,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.external.osxExec": "iTerm.app",
    "git.autofetch": true,
    "gitlens.ai.model": "gitkraken",
    "gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
    "redhat.telemetry.enabled": false,
    "vim.sneak": true,
    "vim.smartRelativeLine": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "<C-r>"
            ],
            "commands": [
                "workbench.action.openRecent"
            ],
            "silent": true
        },
        {
            "before": [
                "<leader>",
                "<C-w>"
            ],
            "commands": [
                "workbench.action.switchWindow"
            ],
            "silent": true
        }
    ],
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "[Dracula Theme Soft]": {
            "button.background": "#009dd1",
            "editor.selectionBackground": "#ffb8f996"
        }
    },
    "editor.tokenColorCustomizations": {
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        },
        "textMateRules": [
            {
                "scope": "keyword.other.dotenv",
                "settings": {
                    "foreground": "#FF000000"
                }
            }
        ]
    },
    "workbench.colorTheme": "Dracula Theme Soft",
    "cursor.composer.shouldAllowCustomModes": true,
    "cursor.composer.shouldChimeAfterChatFinishes": true,
}